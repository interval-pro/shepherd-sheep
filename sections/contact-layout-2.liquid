{% assign margin = section.settings.margin | split: ',' %}
{% assign margin_top = margin[0] %}
{% assign margin_bottom = margin[1] %}
{% assign padding = section.settings.padding | split: ',' %}
{% assign padding_top = padding[0] %}
{% assign padding_bottom = padding[1] %}

{% if section.settings.enable_overlay %}
{% assign section_overlay = 'dt-sc-overlay' %}
{% endif %}

{% if section.settings.show_background_image %}
{% assign lazyScript = "lazyload" %}
{% endif %}

<div class="dt-sc-section-wrapper  {{lazyScript}} {{ section_overlay }} {% if section.settings.background_style == 'parallax' %} parallax-initiated{% endif %}" {% if section.settings.background_style == 'parallax' %} data-speed="2" data-style="parallax"{% endif %} style="{% render 'bg-color-image-padding' %} background-size: {{- section.settings.background_size -}}; {% if section.settings.background_style == 'fixed' %}background-attachment: fixed;{% endif %} background-repeat:{% if section.settings.background_size == 'repeat' %}repeat{% else %}no-repeat;{% endif %}" {% include 'bg_image' %}>
  {% render 'section-heading', class: section.id %}
  {{ page.content }}
  {% if section.settings.contact_section %}
  <div class="store-pickup" style="background-color:{{section.settings.section_bg_1 }}; padding:{{section.settings.section_1_padding }}">
    <div class="{% render 'container-settings' %}">
      <div class="row">
        {% if section.settings.contact_section_title1 != blank %}
        <h3>{{section.settings.contact_section_title1}}</h3>
        {% endif %}
        <ul class="dt-contact-iconblock-section">
          {% if section.settings.contact_section_icon_1 != blank %}         
          <li class="dt-contact-icon-block">
            {% if section.settings.contact_section_icon_1 != blank  %}
            <div class="dt-contact-icon-image">
              <i class="{{section.settings.contact_section_icon_1}}"></i> 
            </div>
            {% endif %}
            <div class="dt-contact-icon-content">
              <h6 class="title">{{ section.settings.contact_section_heading1 }}</h6>
              <p>{{ section.settings.contact_section_description1 }}</p>
            </div>
          </li>
          {% endif %}
          {% if section.settings.contact_section_icon_2 != blank %}         
          <li class="dt-contact-icon-block">
            {% if section.settings.contact_section_icon_2 != blank  %}
            <div class="dt-contact-icon-image">
              <i class="{{section.settings.contact_section_icon_2}}"></i> 
            </div>
            {% endif %}
            <div class="dt-contact-icon-content">
              <h6 class="title">{{ section.settings.contact_section_heading2 }}</h6>
              <p>{{ section.settings.contact_section_description2 }}</p>
            </div>
          </li>
          {% endif %} 

          {% if section.settings.contact_section_icon_3 != blank %}         
          <li class="dt-contact-icon-block">
            {% if section.settings.contact_section_icon_3 != blank  %}
            <div class="dt-contact-icon-image">
              <i class="{{section.settings.contact_section_icon_3}}"></i> 
            </div>
            {% endif %}
            <div class="dt-contact-icon-content">
              <h6 class="title">{{ section.settings.contact_section_heading3 }}</h6>
              <p>{{ section.settings.contact_section_description3 }}</p>
            </div>
          </li>
          {% endif %} 
        </ul>

      </div>
    </div>
  </div>
  {% endif %}

  <div class="layout-type {{section.settings.contact_layout}}">
    {% if section.settings.contact_section_2 %}
    <div style="background-color:{{section.settings.section_bg_2 }}; padding:{{section.settings.section_2_padding }}">
      <div class="{% render 'container-settings' %}"> 
        <div class="row">
          {% if section.settings.contact_heaidng != blank %}
          <h3>{{section.settings.contact_heaidng}}</h3>
          {% endif %}

          <ul class="dt-contact-iconblock-section">

            {% if section.settings.contact_section_icon_4 != blank %}         
            <li class="dt-contact-icon-block">
              {% if section.settings.contact_section_icon_4 != blank  %}
              <div class="dt-contact-icon-image">
                <i class="{{section.settings.contact_section_icon_4}}"></i> 
              </div>
              {% endif %}
              <div class="dt-contact-icon-content">
                <h6 class="title">{{ section.settings.contact_section_heading4 }}</h6>
                <p>{{ section.settings.contact_section_description4 }}</p>
              </div>
            </li>
            {% endif %}

            {% if section.settings.contact_section_icon_5 != blank %}         
            <li class="dt-contact-icon-block">
              {% if section.settings.contact_section_icon_5 != blank  %}
              <div class="dt-contact-icon-image">
                <i class="{{section.settings.contact_section_icon_5}}"></i> 
              </div>
              {% endif %}
              <div class="dt-contact-icon-content">
                <h6 class="title">{{ section.settings.contact_section_heading5 }}</h6>
                <p>{{ section.settings.contact_section_description5 }}</p>
              </div>
            </li>
            {% endif %} 

            {% if section.settings.contact_section_icon_6 != blank %}         
            <li class="dt-contact-icon-block">
              {% if section.settings.contact_section_icon_6 != blank  %}
              <div class="dt-contact-icon-image">
                <i class="{{section.settings.contact_section_icon_6 }}"></i> 
              </div>
              {% endif %}
              <div class="dt-contact-icon-content">
                <h6 class="title">{{ section.settings.contact_section_heading6 }}</h6>
                <p>{{ section.settings.contact_section_description6 }}</p>
              </div>
            </li>
            {% endif %} 

          </ul>
        </div>
      </div>
    </div>
    {% endif %}

    <div class="map_section" style="background-color:{{section.settings.section_3_bg }}; padding:{{section.settings.section_3_padding }}">
      <div class="{% render 'container-settings' %}"> 
        <div class="row">
          {% if section.settings.contact_section_3 %}
          <div id="map">{{ section.settings.contact_iframe_code }} </div>
          {% endif %}
        </div>
      </div>
    </div>

    <div class="contact_form_bg" style="background-color:{{section.settings.form_bg_color }}; padding:{{section.settings.form_padding }}">
      <div class="{% render 'container-settings' %}"> 
        <div class="row">
          {% if section.settings.enable_contact_form or section.settings.enable_delivery_block %}
          <div class="form_wrapper dt-sc-column two-column">
            <div class="contact-form-section">
              <div class="form_bg" {% if section.settings.form_bg != blank %}style="background-image:url('{{section.settings.form_bg | img_url: 'master'}}');background-size:cover;background-repeat:no-repeat;"{% endif %}>
                {% if section.settings.contact_form_title != blank %}
                <h4> {{ section.settings.contact_form_title }} </h4>
                {% endif %} 
                
                {% form 'contact' %}
                {% if form.posted_successfully? %}
                <p class="note form-success"> {{ 'contact.form.post_success' | t }} </p>
                {% endif %}
                {{ form.errors | default_errors }}         
                <label for="ContactFormName" class="label--hidden">{{ 'contact.form.name' | t }}</label>
                <input type="text" id="ContactFormName" class="input-full" name="contact[name]" placeholder="{{ 'contact.form.name' | t }}" autocapitalize="words" value="{% if form.name %}{{ form.name }}{% elsif customer %}{{ customer.name }}{% endif %}">
                <label for="ContactFormEmail" class="label--hidden">{{ 'contact.form.email' | t }}</label>
                <input type="email" id="ContactFormEmail" class="input-full" name="contact[email]" placeholder="{{ 'contact.form.email' | t }}" autocorrect="off" autocapitalize="off" value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}">
                <label for="ContactFormPhone" class="label--hidden">{{ 'contact.form.phone' | t }}</label>
                <input type="tel" id="ContactFormPhone" class="input-full" name="contact[phone]" placeholder="{{ 'contact.form.phone' | t }}" pattern="[0-9\-]*" value="{% if form.phone %}{{ form.phone }}{% elsif customer %}{{ customer.phone }}{% endif %}">
                <label for="ContactFormMessage" class="label--hidden">{{ 'contact.form.message' | t }}</label>
                <textarea rows="10" id="ContactFormMessage" class="input-full" name="contact[body]" placeholder="{{ 'contact.form.message' | t }}">{% if form.body %}{{ form.body }}{% endif %}</textarea>
                <button type="submit" class="btn">
                  {{ 'contact.form.send' | t }} 
                </button>
                {% endform %}
              </div>
            </div>
            <div class="delivery_block">
              <div class="delivery_block_content" {% if section.settings.delivery_bg != blank %}style="background-image:url('{{section.settings.delivery_bg | img_url: 'master'}}');background-size:cover;background-repeat:no-repeat;"{% endif %}>
                {% if section.settings.delivery_title != blank %}
                <h4>{{section.settings.delivery_title}}</h4>
                {% endif %}
                {% if section.settings.delivery_text != blank %}
                <p>{{section.settings.delivery_text}}</p>
                {% endif %}
                {% if section.settings.delivery_html != blank %}
                <div class="rte">{{section.settings.delivery_html}}</div>
                {% endif %}
              </div>
              {% if section.settings.delivery_banner != blank %}
              <div class="delivery_banner">
                <img src="{{section.settings.delivery_banner | img_url: 'master' }}" />
              </div>
              {% endif %}
            </div>
          </div>
          {% endif %}
        </div>
      </div>
    </div>
  </div>
</div>

<style>
  
  #shopify-section-{{section.id}}.contact-layout-style-2 h3{ margin:0 0 15px;}
  body#contact .breadcrumb{margin-bottom:0 !important;}
  #shopify-section-{{section.id}}.contact-layout-style-2 .dt-sc-heading { margin-bottom: 35px; }
  #shopify-section-{{section.id}}.contact-layout-style-2 .dt-sc-heading .dt-sc-main-heading { color: var(--DTColor_Heading); color:{{ section.settings.section_heading_color }}; } 
  #shopify-section-{{section.id}}.contact-layout-style-2 .dt-sc-heading .dt-sc-sub-heading { color: var(--DTColor_Heading); color:{{ section.settings.section_sub_heading_color }}; } 
  #shopify-section-{{section.id}}.contact-layout-style-2 .dt-sc-heading .dt-sc-heading-description { color: var(--DTColor_Body);  color:{{ section.settings.section_description_color }}; } 
  #shopify-section-{{section.id}}.contact-layout-style-2 .dt-sc-heading .dt-sc-btn { 
    background: var(--DT_Button_BG_Color); background:{{ section.settings.section_button_bg_color }}; 
      color: var(--DT_Button_Text_Color); color:{{ section.settings.section_button_text_color }}; }
  #shopify-section-{{section.id}}.shopify-section-contact-layout-2 .dt-sc-heading .dt-sc-btn:hover { 
    background: var(--DT_Button_BG_Hover_Color); background:{{ section.settings.section_button_hover_bg_color }}; 
      color: var(--DT_Button_Text_Hover_Color); color:{{ section.settings.section_button_hover_text_color }}; }  

  #shopify-section-{{section.id}}.contact-layout-style-2 .dt-sc-overlay:before  { 
    color: var(--DTTertiaryColor); 
      color: {{ section.settings.background_color }};    
      background: currentcolor; 
      opacity: {{ section.settings.overlay_transparency }}; }

  .dt-contact-iconblock-section { 
    margin: 0;
    display: grid;
    grid-template-columns: repeat( auto-fit, minmax(300px, 1fr) );
    gap: var(--DTGutter_Width);
      }
  .dt-contact-iconblock-section .dt-contact-icon-block { 
    border-radius:var(--DTRadius);
      display: flex;
      align-items: flex-start;
      padding: 15px;
      border: 1px solid {{section.settings.border_color}};
      transition: var(--DTBaseTransition); 
        }

  .dt-contact-iconblock-section .dt-contact-icon-block .dt-contact-icon-content { padding-left:20px;}
  .dt-contact-iconblock-section .dt-contact-icon-block .dt-contact-icon-content .title{ margin:0; color: {{section.settings.heading_color}};  }
  .dt-contact-iconblock-section .dt-contact-icon-block .dt-contact-icon-content p{ margin:0; color: {{section.settings.description_color}}; }
  .dt-contact-iconblock-section .dt-contact-icon-block .dt-contact-icon-image > i { 
    position: relative; width: 60px; height: 60px; display: flex; align-items: center;
    justify-content: center;
    transition: var(--DTBaseTransition);
      background: var(--DT_Button_BG_Color);
        color: var(--DT_Button_Text_Color);
          background: {{ section.settings.contacticon_bg_color}}; 
          color:{{ section.settings.contacticon_color}};
          border-radius: var(--DTRadius);
            }
  .dt-contact-iconblock-section .dt-contact-icon-block:hover .dt-contact-icon-image > i { background: var(--DT_Button_BG_Hover_Color); background: {{ section.settings.contacticon_hoverbg_color}}; color: var(--DT_Button_Text_Hover_Color); color:{{ section.settings.contacticonhover_color}}; }

  .layout-type > .contact-form-section h4 { margin-top: 10px; }
  .layout-type h3{padding-bottom:0;}
  .layout-type form.contact-form { display: grid; grid-template-columns: 1fr 1fr 1fr; column-gap: var(--DTGutter_Width); }
  .layout-type form.contact-form > *{ margin-bottom:34px;border-radius:var(--DTRadius);padding:15px 20px;}
  .layout-type form.contact-form textarea#ContactFormMessage {grid-column:1/4;height:180px;}
  .layout-type form.contact-form button { grid-column: 2/3; margin: 0; width: 100%;background:var(--DTColor_Body);border-radius:var(--DTRadius);} 
  .layout-type form.contact-form button:hover{background:var(--DT_Button_BG_Hover_Color);}
  .layout-type form.contact-form .note.form-success { grid-column-start: 1; grid-column-end: 4; margin-bottom: 0; }

  .layout-type .form_wrapper .form_bg { padding:50px;border-radius:var(--DTRadius);position: relative; z-index: 0;border:1px solid {{section.settings.border_color}}; }
  .layout-type .form_wrapper form.contact-form{display:block;}
  .layout-type .form_wrapper .contact-form-section h3{margin-top:0;}
  .layout-type .form_wrapper .delivery_block .delivery_block_content{ position: relative; z-index: 0; padding:50px;border:1px solid {{section.settings.border_color}};border-radius:var(--DTRadius);margin-bottom:var(--DTGutter_Width);}
  .layout-type .form_wrapper .delivery_block h3{margin:0 0 20px;}
  .layout-type .form_wrapper .delivery_block .rte{ border-top:1px solid {{ section.settings.border_color }};padding-top:20px;margin-top:20px;}
  .layout-type > * { flex: 1 1 100%; }

  .layout-type .form_wrapper .form_bg:before, 
  .delivery_block_content:before{ content:''; position:absolute; left:0; right:0; top:0; bottom:0; opacity: .8; z-index: -1; background-color:var(--DTBodyBGColor); }
  .delivery_banner{ text-align: center; }

  /*  Responsive  */
  @media only screen and (min-width: 1200px) {
    #shopify-section-{{section.id}} .dt-sc-section-wrapper { margin-top:{{margin_top}}px; margin-bottom:{{margin_bottom}}px;padding-top:{{padding_top}}px; padding-bottom:{{padding_bottom}}px; }
  }

  @media only screen and (max-width: 1199px) {
    #shopify-section-{{section.id}} .dt-sc-section-wrapper { margin-top:{{margin_top | divided_by: 2 }}px; margin-bottom:{{margin_bottom | divided_by: 2}}px;padding-top:{{padding_top | divided_by: 2}}px; padding-bottom:{{padding_bottom | divided_by: 2}}px;}
    .layout-type form.contact-form > *{padding:12px 20px;margin-bottom:var(--DTGutter_Width);}
    .layout-type > *{padding-left:var(--DTGutter_Width);padding-right:var(--DTGutter_Width);}
  }

  @media only screen and (max-width: 767px) {
    .layout-type .form_wrapper{display:block}
    .layout-type .form_wrapper .contact-form-section{width:100%;}
    .layout-type .form_wrapper .delivery_block{width:78%;}
    .layout-type .form_wrapper .form_bg{padding:30px;}
    .layout-type .form_wrapper .delivery_block .delivery_block_content{padding:30px;}
  }

  @media only screen and (max-width: 576px) {
    .layout-type form.contact-form > input, 
    .layout-type form.contact-form textarea#ContactFormMessage { grid-column: 1/4; }
    .layout-type form.contact-form button { grid-column: 1/2; min-width: 150px; }
    .layout-type .form_wrapper .delivery_block{width: 100%;}
  }  

</style>

