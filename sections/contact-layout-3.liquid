                        .cir1{color:currentcolor;}
                        .clk0{color:transparent;}
                        .clock{border-radius:50%;border: 1px solid currentcolor;}
                        @media not all and (min-resolution:.001dpcm) { 
                          @supports (-webkit-appearance:none) {
                            .clk0{color:var(--DT_Button_Text_Color);}
