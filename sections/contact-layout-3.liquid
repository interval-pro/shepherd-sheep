                      <circle  class="cir1"cx="40.9" cy="23.8" r="1.4"/>
                      <circle  class="cir1"cx="6.8" cy="23.8" r="1.4"/>
                      <circle  class="cir1"cx="23.9" cy="40.7" r="1.4"/>
                      <rect  class="cir1"x="23.1" y="11.7" width="1.6" height="13"/>
                      <rect  class="cir1" x="23.3" y="21" transform="matrix(0.7578 -0.6525 0.6525 0.7578 -7.7728 22.6874)" width="6.8" height="1.6"/>
