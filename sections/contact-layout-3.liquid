                  <p> 
                    <a title="" href="mailto:{{ section.settings.contact_module_email1 }}">{{ section.settings.contact_module_email1 }}</a><br>
                    <a title="" href="mailto:{{ section.settings.contact_module_email2 }}">{{ section.settings.contact_module_email2 }}</a>
                  </p> 
                </div>
              </li>
              {% endif %}        
              {% if section.settings.contact_opening_hours %} 
              <li class="dt-contact-icon-block"> 
                <div class="dt-contact-icon-image">
                  <i>
                    <svg class="clock" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
