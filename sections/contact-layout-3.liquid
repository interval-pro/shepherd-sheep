                        }
                      </style>
                      </i>
                    </div> 
                  <div class="dt-contact-icon-content">
                    <h5>{{ section.settings.contact_opening__title }}</h5>
                    <p> {{ section.settings.contact_opening_hours1 }}
                  </div>
                  </li>
                {% endif %}
            </ul>
          </div>
          {% endif %}

          {% unless section.settings.form_style_2 %}

          {% if section.settings.enable_contact_form %}
          <div class="contact-form-section">

            <div class="dt-sc-heading">
              <h4> {{ section.settings.contact_form_title }} </h4>
              <p> {{ section.settings.title_description }}</p>
            </div>

            {% form 'contact' %}
            {% if form.posted_successfully? %}
            <p class="note form-success">
              {{ 'contact.form.post_success' | t }}
            </p>
            {% endif %}
            {{ form.errors | default_errors }}         
            <label for="ContactFormName" class="label--hidden">{{ 'contact.form.name' | t }}</label>
            <input type="text" id="ContactFormName" class="input-full" name="contact[name]" placeholder="{{ 'contact.form.name' | t }}" autocapitalize="words" value="{% if form.name %}{{ form.name }}{% elsif customer %}{{ customer.name }}{% endif %}">
            <label for="ContactFormEmail" class="label--hidden">{{ 'contact.form.email' | t }}</label>
            <input type="email" id="ContactFormEmail" class="input-full" name="contact[email]" placeholder="{{ 'contact.form.email' | t }}" autocorrect="off" autocapitalize="off" value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}">
            <label for="ContactFormPhone" class="label--hidden">{{ 'contact.form.phone' | t }}</label>
            <input type="tel" id="ContactFormPhone" class="input-full" name="contact[phone]" placeholder="{{ 'contact.form.phone' | t }}" pattern="[0-9\-]*" value="{% if form.phone %}{{ form.phone }}{% elsif customer %}{{ customer.phone }}{% endif %}">
            <label for="ContactFormMessage" class="label--hidden">{{ 'contact.form.message' | t }}</label>
            <textarea rows="6" id="ContactFormMessage" class="input-full" name="contact[body]" placeholder="{{ 'contact.form.message' | t }}">{% if form.body %}{{ form.body }}{% endif %}</textarea>
            <div class="btn-submit">
              <button type="submit" class="btn">
                {{ 'contact.form.send' | t }} 
              </button> </div>
            {% endform %}          
            {% endif %}
          </div>
          {% endunless %}

          {% if section.settings.form_style_2 %}

          {% if section.settings.enable_contact_form %}
          <div class="contact-form-section">

            <div class="dt-sc-heading">
              <h4> {{ section.settings.contact_form_title }} </h4>
              <p> {{ section.settings.title_description }}</p>
            </div>

            {% form 'contact' %}
            {% if form.posted_successfully? %}
            <p class="note form-success">
              {{ 'contact.form.post_success' | t }}
            </p>
            {% endif %}
            {{ form.errors | default_errors }}         
            <div>
              <label for="ContactFormName" class="label--hidden">{{ 'contact.form.name' | t }} <i class="fa fa-user-o" aria-hidden="true"></i> </label>
              <input type="text" id="ContactFormName" class="input-full" name="contact[name]" placeholder="{{ 'contact.form.name' | t }}" autocapitalize="words" value="{% if form.name %}{{ form.name }}{% elsif customer %}{{ customer.name }}{% endif %}">
            </div><div>
            <label for="ContactFormEmail" class="label--hidden">{{ 'contact.form.email' | t }} <i class="fa fa-envelope-o" aria-hidden="true"></i></label>
            <input type="email" id="ContactFormEmail" class="input-full" name="contact[email]" placeholder="{{ 'contact.form.email' | t }} " autocorrect="off" autocapitalize="off" value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}">
            </div><div>
            <label for="ContactFormPhone" class="label--hidden">{{ 'contact.form.phone' | t }} <i class="fa fa-phone" aria-hidden="true"></i></label>
            <input type="tel" id="ContactFormPhone" class="input-full" name="contact[phone]" placeholder="{{ 'contact.form.phone' | t }} " pattern="[0-9\-]*" value="{% if form.phone %}{{ form.phone }}{% elsif customer %}{{ customer.phone }}{% endif %}">
            </div>
            <div class="text-item">
              <label for="ContactFormMessage" class="label--hidden">{{ 'contact.form.message' | t }}  <i class="fa fa-comment-o" aria-hidden="true"></i> </label>
              <textarea rows="5" id="ContactFormMessage" class="input-full" name="contact[body]" placeholder="{{ 'contact.form.message' | t }}">{% if form.body %}{{ form.body }}{% endif %}</textarea>
            </div><div>
            <button type="submit" class="btn">
              {{ 'contact.form.send' | t }} 
            </button></div>
            {% endform %}          
            {% endif %}
          </div>
          {% endif %}


        </div>
      </div>
    </div>
  </div>
</div>

<style>

  #shopify-section-{{section.id}}.contact-layout-3 .dt-sc-section-wrapper { z-index: 0;}

  #shopify-section-{{section.id}}.contact-layout-3 .dt-sc-overlay:before  { 
    color: var(--DTTertiaryColor); 
      color: {{ section.settings.background_color }};    
      background: currentcolor; 
      opacity: {{ section.settings.overlay_transparency }}; }

  #shopify-section-{{section.id}}.contact-layout-3 .dt-sc-heading { margin-bottom: 35px; }
  #shopify-section-{{section.id}}.contact-layout-3 .dt-sc-heading .dt-sc-main-heading { color: var(--DTColor_Heading); color:{{ section.settings.section_heading_color }}; } 
  #shopify-section-{{section.id}}.contact-layout-3 .dt-sc-heading .dt-sc-sub-heading { color: var(--DTColor_Heading); color:{{ section.settings.section_sub_heading_color }}; } 
  #shopify-section-{{section.id}}.contact-layout-3 .dt-sc-heading .dt-sc-heading-description { color: var(--DTColor_Body);  color:{{ section.settings.section_description_color }}; } 
  #shopify-section-{{section.id}}.contact-layout-3 .dt-sc-heading .dt-sc-btn { 
    background: var(--DT_Button_BG_Color); background:{{ section.settings.section_button_bg_color }}; 
      color: var(--DT_Button_Text_Color); color:{{ section.settings.section_button_text_color }}; }
  #shopify-section-{{section.id}}.contact-layout-3 .dt-sc-heading .dt-sc-btn:hover { 
    background: var(--DT_Button_BG_Hover_Color); background:{{ section.settings.section_button_hover_bg_color }}; 
      color: var(--DT_Button_Text_Hover_Color); color:{{ section.settings.section_button_hover_text_color }}; }  

  /*Old style*/

  #shopify-section-{{section.id}}.contact-layout-3 .dt-contact-icon-block-section { margin: 0; }
  #shopify-section-{{section.id}}.contact-layout-3 .dt-contact-icon-block-section .dt-contact-icon-block { 
    /*     border-width: 1px; */
    /*     border-style: solid; */
    border-color: {{ settings.border_color}};
    border-color: {{ section.settings.contactborder_color }}; 
    padding: {{ section.settings.section_contact_padding }}; 
    transition: var(--DTBaseTransition); 
      border-radius: var(--DTRadius);
        }

  #shopify-section-{{section.id}}.contact-layout-3  .dt-contact-icon-block-section .dt-contact-icon-block .dt-contact-icon-content br { display: block; margin-top: 6px; }
  #shopify-section-{{section.id}}.contact-layout-3  .dt-contact-icon-block-section .dt-contact-icon-block:hover { border-color: var(--DTPrimaryColor); border-color: {{ section.settings.contact_hvrborder_color }};}  

  /*Contact style*/ 

  #shopify-section-{{section.id}}.contact-layout-3 .layout-type.left_icon-block_form_inline .cont-detail-main .dt-sc-heading { text-align: {{ section.settings.contact_heading_position }}; width: 100%;}

  #shopify-section-{{section.id}}.contact-layout-3 .layout-type.left_icon-block_form_inline .cont-detail-main .dt-sc-heading .cont-main-title { 
    color: var(--DTColor_Heading); color: {{section.settings.contact_det_title_color}};}
  #shopify-section-{{section.id}}.contact-layout-3 .layout-type.left_icon-block_form_inline .cont-detail-main .dt-sc-heading .cont-main-description { 
    color: var(--DTColor_Heading); color: {{section.settings.contact_det_description_color}};}

  #shopify-section-{{section.id}}.contact-layout-3 .layout-type.left_icon-block_form_inline .cont-detail-main .dt-contact-icon-block .dt-contact-icon-image svg { 
    width: 30px; height: 30px;}

  #shopify-section-{{section.id}}.contact-layout-3 .layout-type.left_icon-block_form_inline .cont-detail-main .dt-contact-icon-block .dt-contact-icon-content p a { 
    color: var(--DTLinkColor); color: {{section.settings.mail_color}}; }
  #shopify-section-{{section.id}}.contact-layout-3 .layout-type.left_icon-block_form_inline .cont-detail-main .dt-contact-icon-block .dt-contact-icon-content p a:hover { 
    color: var(--DTLinkHoverColor); color: {{section.settings.mail_hover_color}}; }

