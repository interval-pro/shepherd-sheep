.dt-border-line {
  float: left;
  width: 100%;
  border: 2px solid var(--DTColor_Border);
}

.user-account {
  position: relative;
  max-width: 900px;
  margin: auto auto 30px;
  display: flex;
  background-color: var(--DTform_BG);
  border-radius: 1rem;
  overflow: hidden;
  margin: 5rem auto;
}

.user-account .grid__item {
  width: 50%;
  padding: 2rem;
}

.user-account .auth-container-left-side {
  background-color: #C23E24;
  color: var(--color-5);
  display: flex;
  flex-direction: column;
  justify-content: center !important;
  align-items: center;
  text-align: center;
  gap: 2rem;
}

.user-account .auth-container-left-side h5 {
  color: var(--color-5);
  font-weight: bold;
}

.user-account .auth-container-left-side .p-button {
  background-color: unset;
  border: 2px solid var(--color-5);
}
.user-account .auth-container-left-side .p-button a {
  color: var(--color-5);
}

.user-account .auth-container-left-side .p-button:hover {
  background-color: var(--color-3);
  color: var(--DTform_BG);
}

.user-account .auth-container-right-side form {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.user-account .auth-container-right-side h5 {
  font-weight: bold;
}

.user-account .auth-container-right-side form input[type=email],
.user-account .auth-container-right-side form input[type=password],
.user-account .auth-container-right-side form input[type=text] {
  background-color: unset;
  border: 2px solid var(--DTColor_Heading);
  border-radius: var(--DT_Button_Border_Radius);
}

.user-account .auth-container-right-side form a {
  text-decoration: underline;
  color: var(--DTPrimaryColor);
  margin-bottom: 3rem;
}

.user-account .grid__item>div {
  width: 100%;
  justify-content: center;
  display: flex;
  align-items: center;
}

.user-account .grid__item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.user-account .grid__item.address_check {
  margin-bottom: 30px;
}

.user-account form {
  float: left;
  position: relative;
  width: 100%;
}

.user-account form label {
  width: 100%;
  float: left;
}

.user-account form input {
  margin-bottom: var(--DTGutter_Width);
}

.user-account form input.btn,
.user-account form p:last-child {
  margin: 0 auto;
  text-align: center;
}

.user-account form .btn.dt-sc-btn {
  margin: 0;
}

.user-account form .btn.dt-sc-btn+.dt-sc-btn {
  margin-left: 15px;
}

.user-account form input[type=checkbox] {
  margin: 2px 10px 5px 0;
}

.user-account .dt-sc-btn-group {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(220px, 1fr));
